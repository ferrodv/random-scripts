{
  "name": "pmt-syncer",
  "version": "1.0.0",
  "main": "handler.js",
  "description": "Mini app that syncs card from Jira to Clickup",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development FUNCTIONSFILE=local serverless offline --noTimeout",
    "start:prod": "NODE_ENV=production FUNCTIONSFILE=cloud serverless offline",
    "deploy": "NODE_ENV=production serverless deploy",
    "test": "NODE_ENV=test jest --verbose --silent --detectOpenHandles --forceExit",
    "test:long": "NODE_ENV=test jest --verbose --detectOpenHandles --forceExit --testTimeout 100000",
    "linter": "eslint ./src",
    "linter:fix": "eslint ./src --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.1",
    "babel-loader": "^9.1.2",
    "cache-loader": "^4.1.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "serverless": "^3.31.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^12.0.4",
    "serverless-webpack": "^5.11.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.84.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "dotenv": "^16.0.3"
  }
}
